{
  "name": "travelcollabapp",
  "version": "1.0.0",
  "description": "A travel app that attempts to solve multiple problems with traval applications.",
  "main": "server.js",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "concurrently \"nodemon server.js\" \"npm run client\"",
    "build": "node scripts/build.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "node scripts/build.js",
    "heroku-prebuild": "cd client && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mijofields/TravelCollabApp.git"
  },
  "author": "Mike Fields, Dewi Nurdin, Kristen Watson, Trey Hughes, Jacolby Green",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mijofields/TravelCollabApp/issues"
  },
  "homepage": "https://github.com/mijofields/TravelCollabApp#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "googleapis": "^28.1.0",
    "jsonwebtoken": "^8.2.1",
    "jwt-decode": "^2.2.0",
    "mkdirp": "^0.5.1",
    "mongoose": "^5.0.14",
    "morgan": "^1.9.0",
    "path": "^0.12.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "react-stripe-checkout": "^2.6.3",
    "socket.io": "^2.1.0",
    "stripe": "^5.8.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.3"
  }
}
